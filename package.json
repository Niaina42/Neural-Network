{
  "name": "henon-series",
  "version": "0.1.1",
  "author": {
    "name": "Niaina Christopher IGGLIA4 nÂ°18",
    "email": "niainachristopher4@gmail.com"
  },
  "description": "Artificial Neural Network Project with Electron JS and React (Henon series)",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-charts": "^7.7.0",
    "@mui/x-data-grid": "^7.7.0",
    "@tsparticles/engine": "^3.4.0",
    "@tsparticles/react": "^3.0.0",
    "mathjs": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "tsparticles": "^3.4.0",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "wait-on tcp:3000 && electron .",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "dist:win": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js --win",
    "dist:linux": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js --linux"
  },
  "build": {
    "appId": "com.devdegany.henonseries",
    "productName": "Henon Series RNA",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraResources": []
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ajv": "^7.2.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "wait-on": "^7.2.0"
  }
}
